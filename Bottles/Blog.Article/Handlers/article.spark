<viewdata model="Blog.Article.ArticleViewModel"/>
<article>
    <header>
      <h2>
        ${Model.Title}
      </h2>
      <time datetime="${Model.PublishedDate}">
        Posted On: ${Model.PublishedDateString} by
      </time> <a href="/author" rel="author">${Model.Author}</a>
    </header>
    <section>
      !{Model.Body}
    </section>
    <p>
    </p>
</article>